<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <!-- com.gto.zero.zboost.function.gameboost.view.GameAccelLoadingView -->
    <declare-styleable name="GameAccelLoadingView">
        <attr name="loading_width" format="dimension" />
        <attr name="background_color" format="color" />
    </declare-styleable>

</resources>