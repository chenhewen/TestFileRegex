<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/boost_disable_apps_app_icon_size"
    android:layout_height="@dimen/boost_disable_apps_app_icon_size" >

    <ImageView
        android:id="@+id/boost_disable_apps_app_icon_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:contentDescription="@null"
        android:padding="8dp" />

    <ImageView
        android:id="@+id/boost_disable_apps_ice_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:contentDescription="@null"
        android:src="@drawable/boost_disable_apps_anim_ice" />

</FrameLayout>