<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:wheel="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/common_item_alternative"
                tools:context="com.gto.zero.zboost.function.clean.activity.CleanIgnoreActivity">

    <include
        android:id="@+id/clean_ignore_title"
        layout="@layout/common_title_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:background="@color/common_title_background"/>

    <com.gto.zero.zboost.common.ui.floatlistview.FloatingGroupExpandableListView
        android:id="@+id/clean_ignore_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/clean_ignore_title"
        android:cacheColorHint="#00000000"
        android:childDivider="@color/common_listview_divider"
        android:divider="#00000000"
        android:fadingEdge="none"
        android:listSelector="#00000000"
        android:overScrollFooter="@null"
        android:overScrollMode="never"
        android:scrollbars="none"/>

    <include
        android:id="@+id/clean_ignore_empty"
        layout="@layout/common_empty_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"/>


    <com.gto.zero.zboost.view.ProgressWheel
        android:id="@+id/clean_ignore_loading"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_centerInParent="true"
        wheel:matProg_barColor="@color/common_green_normal"
        wheel:matProg_barWidth="3dp"
        wheel:matProg_progressIndeterminate="true"/>

</RelativeLayout>
