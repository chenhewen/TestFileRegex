<?xml version="1.0" encoding="utf-8"?>
<com.gto.zero.zboost.common.ui.ZoomRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="108dp"
    android:layout_height="108dp">

    <ImageView
        android:id="@+id/power_saving_main_boost_btn_halo"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:src="@drawable/power_saving_main_btn_halo"/>

    <View
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_centerInParent="true"
        android:background="@drawable/power_saving_main_btn_bg"/>

    <ImageView
        android:id="@+id/power_saving_main_boost_btn_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:src="@drawable/power_saving_main_btn_rocket"/>


</com.gto.zero.zboost.common.ui.ZoomRelativeLayout>
