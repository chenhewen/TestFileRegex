<?xml version="1.0" encoding="utf-8"?>
<com.gto.zero.zboost.function.boost.accessibility.disable.view.AppsDisableProcessAnimPage xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/boost_disable_apps_process_layout_bg" >

    <FrameLayout
        android:id="@+id/boost_disable_apps_anim_icons_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/boost_disable_apps_anim_cloud_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:contentDescription="@null"
                android:src="@drawable/boost_disable_apps_anim_cloud" />

            <TextView
                android:id="@+id/boost_disable_apps_wait_to_stop_tips_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/boost_disable_apps_anim_cloud_view"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="16dp"
                android:fontFamily="@string/font_families_roboto"
                android:singleLine="true"
                android:text="@string/disable_apps_wait_to_stop_tips"
                android:textColor="#FFFFFFFF"
                android:textSize="16dp"
                android:textStyle="normal" />
        </RelativeLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="131dp" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/boost_disable_apps_anim_app_name_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@string/font_families_roboto"
                    android:singleLine="true"
                    android:text="Clean Master"
                    android:textColor="#FFFFFFFF"
                    android:textSize="16dp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/boost_disable_apps_anim_tips_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="4dp"
                    android:fontFamily="@string/font_families_roboto"
                    android:singleLine="true"
                    android:text="@string/disable_apps_process_anim_tips"
                    android:textColor="#80FFFFFF"
                    android:textSize="16dp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/boost_disable_apps_anim_process_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@string/font_families_roboto"
                    android:singleLine="true"
                    android:text="2/9"
                    android:textColor="#FFFFFFFF"
                    android:textSize="24dp"
                    android:textStyle="normal" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>

</com.gto.zero.zboost.function.boost.accessibility.disable.view.AppsDisableProcessAnimPage>