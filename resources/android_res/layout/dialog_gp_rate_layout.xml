    android:layout_height="match_parent"
    android:background="#FFFFFFFF" >

    <com.gto.zero.zboost.view.ScrollPager
        android:id="@+id/rate_dialog_scrollpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <FrameLayout
            android:id="@+id/dialog_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
