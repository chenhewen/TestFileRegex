<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="58dp"
    android:layout_height="wrap_content" >
    
    <ImageView 
        android:id="@+id/fragment_filecategory_sec_list_header_colortext_color"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:layout_centerVertical="true"
        />
    
    <TextView 
        android:id="@+id/fragment_filecategory_sec_list_header_colortext_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/fragment_filecategory_sec_list_header_colortext_color"
        android:layout_marginLeft="6dp"
        android:textColor="#89FFFFFF"
        android:textSize="12dp"
        android:layout_centerVertical="true"/>

</RelativeLayout>
