<?xml version="1.0" encoding="utf-8"?>
<com.gto.zero.zboost.function.filecategory.deepclean.common.view.CommonDeepCleanView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#f5f7fa">

    <RelativeLayout
        android:id="@+id/common_deep_clean_item_view_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:background="@color/white">

        <RelativeLayout
            android:id="@+id/common_deep_clean_item_view_left"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp">


            <TextView
                android:id="@+id/common_deep_clean_item_view_left_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="50dp"
                android:textColor="#DD000000"
                android:textSize="14dp"
                tools:text="Music"
                />

            <TextView
                android:id="@+id/common_deep_clean_item_view_left_occupy_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/common_deep_clean_item_view_left_title"
                android:layout_centerHorizontal="true"
                android:textColor="#89000000"
                android:textSize="12dp"
                tools:text="2.56GB"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/common_deep_clean_item_view_click_wrapper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/common_deep_clean_item_view_left"
            android:background="@drawable/filcategory_sec_item_bg_selector">

            <RelativeLayout
                android:id="@+id/common_deep_clean_item_view_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/storage_new_next_btn"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/common_deep_clean_item_view_middle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_toLeftOf="@+id/common_deep_clean_item_view_right"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="208dp">

                    <include
                        android:id="@+id/common_deep_clean_item_view_middle_content_one"
                        layout="@layout/common_deep_clean_item_view_item"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        />

                    <include
                        android:id="@+id/common_deep_clean_item_view_middle_content_two"
                        layout="@layout/common_deep_clean_item_view_item"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        />

                    <include
                        android:id="@+id/common_deep_clean_item_view_middle_content_three"
                        layout="@layout/common_deep_clean_item_view_item"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerVertical="true"
                        />

                </RelativeLayout>


            </RelativeLayout>

        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="113dp"
            android:layout_toRightOf="@+id/common_deep_clean_item_view_left">

            <com.gto.zero.zboost.function.filecategory.view.FileCategoryItemProcessView
                android:id="@+id/common_deep_clean_item_view_loading"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16dp"
                android:visibility="gone"
                />

        </RelativeLayout>


    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/common_deep_clean_item_view_container"
        android:background="@color/white"
        android:orientation="vertical"
        android:paddingLeft="92dp">

    </LinearLayout>


    <com.gto.zero.zboost.function.filecategory.deepclean.common.view.CommonDeepCleanIconView
        android:id="@+id/common_deep_clean_item_view_left_icon"
        android:layout_width="68dp"
        android:layout_height="63.75dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"
        />

</com.gto.zero.zboost.function.filecategory.deepclean.common.view.CommonDeepCleanView>

